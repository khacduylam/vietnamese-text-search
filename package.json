{
  "name": "vietnamese-text-search",
  "version": "1.0.2",
  "description": "Text search for vietnamese.",
  "main": "./lib/search.js",
  "scripts": {
    "test": "NODE_ENV=development nodemon --exec babel-node ./src/test/index.js --ignore ./src/test/",
    "build": "rimraf ./lib && babel ./src --out-dir ./lib --ignore ./src/test --copy-files --no-copy-ignored",
    "prepub": "npm run build",
    "pub": "npm run prepublish && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khacduylam/vietnamese-text-search"
  },
  "keywords": [
    "full-text",
    "vietnamese",
    "search"
  ],
