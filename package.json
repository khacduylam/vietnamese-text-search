  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khacduylam/vietnamese-text-search/issues"
  },
  "homepage": "https://github.com/khacduylam/vietnamese-text-search#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "chai": "^4.2.0",
    "exceljs": "^4.2.0",
    "execution-time": "^1.4.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "write-json-file": "^4.3.0"
  }
